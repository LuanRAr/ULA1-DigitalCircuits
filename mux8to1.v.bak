module mux8to1 (Add, Sub, Mul, Div, And, Or, Xor, NotUse, Selector, Y);

input Add, Sub, Mul, Div, And, Or, Xor, NotUse;
input [2:0] Selector;
output muxOut;

wire y1, y2, y3, y4;

mux2to1 mux1(
	.A(Add),
	.B(Sub),
	.Selector(Selector[0]),
	.Y(y1)
);

mux2to1 mux2(
	.A(Mul),
	.B(Div),
	.Selector(Selector[0]),
	.Y(y2)
);

mux2to1 mux3(
	.A(And),
	.B(Or),
	.Selector(Selector[0]),
	.Y(y3)
);

mux2to1 mux4(
	.A(Xor),
	.B(NotUse),
	.Selector(Selector[0]),
	.Y(y4)
);

mux4to1 mux5(
	.A(y1),
	.B(y2),
	.C(y3),
	.D(y4),
	.Selector(Selector[2:1]),
	.Y(Y)
);

endmodule
	

